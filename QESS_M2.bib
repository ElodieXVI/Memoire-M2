@article{breugnot_les_2012,
	title = {Les innovations dans le champ de la protection de l'enfance},
	volume = {N° 57},
	issn = {1957-0341},
	url = {https://www.cairn.info/revue-la-nouvelle-revue-de-l-adaptation-et-de-la-scolarisation-2012-1-page-259.htm},
	abstract = {Issu d’une thèse relative à l’étude des « innovations socio-éducatives » en protection de l’enfance, l’article expose les résultats du croisement d’interrogations émanant d’une pratique de terrain en tant qu’assistante sociale et d’une précédente étude (Breugnot, Durning, 2001) relative à un état des lieux des recherches sur l’action éducative en milieu ouvert. Des entretiens ont été menés dans une trentaine de services auprès de différents membres de l’équipe éducative (directeur, responsables d’équipe, travailleurs sociaux…), et de personnes externes (magistrats, responsables de l’ASE…). À partir de l’analyse systématique réalisée à l’aide d’une grille centrée sur les intentions, le contexte, la mise en œuvre, les évolutions du dispositif, le regard des acteurs sur le projet et des éléments relatifs à l’évaluation des différents services, l’article propose une classification des innovations en cinq types principaux (le relais parental, l’accueil de jour familial, l’accueil de jour de l’enfant, l’accueil séquentiel, la prise en charge associant intervention à domicile et suppléance familiale) puis dégage les principales caractéristiques des innovations faisant l’objet d’un essaimage. Les conditions d’une appropriation des innovations par les acteurs concernés sont mises en avant, et l’on s’interroge sur les effets de la pression à l’efficacité dans des délais de mise en œuvre singulièrement raccourcis.},
	language = {fr},
	number = {1},
	urldate = {2021-05-29},
	journal = {La nouvelle revue de l'adaptation et de la scolarisation},
	author = {Breugnot, Pascale},
	year = {2012},
	note = {Publisher: I.N.S.H.E.A.},
	pages = {259--270},
}

@article{rousseau_devenir_2016,
	title = {Devenir à long terme de très jeunes enfants placés à l’{Aide} sociale à l’enfance},
	issn = {0035-2985},
	url = {https://www.cairn.info/revue-francaise-des-affaires-sociales-2016-1-page-343.htm},
	abstract = {La recherche Saint-Exup\&\#233;ry. 2011-2014 d\&\#233;crit le devenir sur 20 ans de 129 enfants ayant b\&\#233;n\&\#233;fici\&\#233;, avant l\&\#8217;\&\#226;ge de 4 ans, d\&\#8217;un placement dans un \&\#233;tablissement de l\&\#8217;Aide sociale \&\#224; l\&\#8217;enfance (ASE) du d\&\#233;partement de Maine-et-Loire (49). Les biographies exhaustives de ces enfants ont \&\#233;t\&\#233; reconstitu\&\#233;es depuis leur naissance jusqu\&\#8217;\&\#224; leur majorit\&\#233;. La morbidit\&\#233; personnelle \&\#224; l\&\#8217;admission est tr\&\#232;s forte (mauvais \&\#233;tat g\&\#233;n\&\#233;ral, souffrance psychique) dans un climat familial souvent tr\&\#232;s perturb\&\#233; (nombreux enfants plac\&\#233;s dans la fratrie, troubles psychiatriques parentaux). Tous les enfants progressent ensuite, mais en ordre dispers\&\#233;. Les enfants dont la gravit\&\#233; de l\&\#8217;\&\#233;tat \&\#224; l\&\#8217;admission est importante et pour lesquels les d\&\#233;cisions de placement ont \&\#233;t\&\#233; prises apr\&\#232;s un long d\&\#233;lai, progressent le moins bien. La recherche met en \&\#233;vidence plusieurs d\&\#233;terminants p\&\#233;joratifs du devenir, notamment la gravit\&\#233; et la dur\&\#233;e de l\&\#8217;exposition au risque. \&\#192; l\&\#8217;\&\#226;ge adulte, un quart des enfants conservent un handicap psychique et une inadaptation sociale et sont d\&\#233;pendants des prestations sociales. \&\#192; l\&\#8217;oppos\&\#233;, un autre quart \&\#233;volue favorablement\&\#160;; il s\&\#8217;agit d\&\#8217;enfants pris en charge suffisamment t\&\#244;t, avec des troubles somatiques et psychologiques encore r\&\#233;versibles. La recherche met aussi en \&\#233;vidence des facteurs protecteurs, notamment une prise en charge pr\&\#233;coce. Les \&\#233;volutions p\&\#233;joratives paraissent donc pouvoir \&\#234;tre r\&\#233;duites en am\&\#233;liorant le d\&\#233;pistage et en d\&\#233;veloppant cette prise en charge pr\&\#233;coce.},
