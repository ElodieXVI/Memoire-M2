@phdthesis{zogmal_les_2015,
	title = {Les processus d'observation et de catégorisation des enfants comme outil de travail dans les pratiques professionnelles des éducatrices et éducateurs de l'enfance},
	url = {https://archive-ouverte.unige.ch/unige:79060},
	abstract = {Cette recherche s'intéresse aux compétences professionnelles mobilisées par les éducatrices/-teurs de l'enfance pour construire des « connaissances » portant sur les enfants, leurs conduites, leurs compétences et leurs caractéristiques. L'émergence des processus d'observation et de catégorisation dans les pratiques réelles est analysée dans une perspective interactionnelle, à travers quatre axes : 1) L'ostension des processus d'observation et de catégorisation dans les interactions entre éducatrices et enfants ; 2) La dimension collective dans les interactions de l'équipe éducative ; 3) La dimension dynamique à travers l'analyse de deux trajectoires situées ; 4) L'acquisition et la transmission des processus étudiés. Ce travail met en évidence les phénomènes d'ostension et aborde comment les éducatrices/-teurs parlent de leurs observations et des catégorisations effectuées. L'ostension et la présomption d'une intentionnalité s'imbriquent dans un travail d'enquête pour s'ajuster aux enfants. En abordant la banalité des interactions quotidiennes, cette recherche éclaire l'accomplissement d'un travail éducatif.},
	language = {fre},
	urldate = {2021-06-05},
	school = {University of Geneva},
	author = {Zogmal, Marianne},
	year = {2015},
	doi = {10.13097/archive-ouverte/unige:79060},
	doi = {10.13097/archive-ouverte/unige:79060},
	file = {Full Text PDF:/Users/elodielemaire/Zotero/storage/9I27PYXE/Zogmal - 2015 - Les processus d&#039\;observation et de catégorisat.pdf:application/pdf;Snapshot:/Users/elodielemaire/Zotero/storage/ALDQKCJ3/unige79060.html:text/html},
