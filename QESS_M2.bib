@phdthesis{molina_les_2015,
	type = {These de doctorat},
	title = {Les travailleurs sociaux, des groupes professionnels en transformation},
	copyright = {Licence Etalab},
	url = {https://www.theses.fr/2015EHES0055},
	abstract = {L'objet de la recherche porte sur une analyse des processus de professionnalisation de trois groupes professionnels « historiques » du travail social : assistant de service social, éducateur spécialisé, conseiller en économie sociale et familiale. Sont mises au jour des imbrications complexes entre intérêts collectifs et individuels des différents protagonistes. Ces processus rendent compte des recompositions professionnelles à l'œuvre mêlant trois niveaux d'analyse : de nouvelles régulations politiques, administratives et juridiques ; l'introduction de nouvelles logiques managériales au sein des organisations de travail ; et enfin le parcours biographique et professionnel de chaque individu. Depuis les années 1980, le secteur de l'action sociale et médico-sociale connaît des transformations dans un contexte socio-économique et politique lui-même en mutation, marqué par une crise économique durable et structurelle. Les années 2000 se présentent comme une étape majeure dans les transformations de la professionnalisation du secteur à travers des réformes d'ampleur. Au-delà de la codification des activités de travail et des compétences formalisées et standardisées dans des référentiels professionnels, elles visent des mobilités entre les groupes pour un exercice professionnel polyvalent au sein des organisations de travail. Il s'agit, en d'autres termes, de formes de décloisonnement professionnel qui privilégient la mission plutôt que le métier dans le contexte du Nouveau Management Public. Sont analysés dans la recherche, les rapports sociaux dans lesquels se jouent les segments et quels en sont les enjeux et symétriquement les zones de recouvrement.},
	urldate = {2021-06-05},
	school = {Paris, EHESS},
	author = {Molina, Yvette},
	collaborator = {Grelon, André},
	month = jan,
	year = {2015},
}

